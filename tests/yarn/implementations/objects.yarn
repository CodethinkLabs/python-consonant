    NEW_OBJECTS=$(created_objects master)
    run_python_test <<-EOF
    new_object = eval("$NEW_OBJECTS")[0]
    if isinstance(output_yaml, dict):
      objects = []; [objects.extend(x) for x in output_yaml.itervalues()]
    else:
      objects = output_yaml
